<Page x:Class="VirusWPF.Pages.WelcomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VirusWPF.Pages"
      xmlns:n="clr-namespace:VirusWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WelcomePage">

    <Grid Background="White" ButtonBase.Click="Grid_Click">
        <WrapPanel Margin="5">
            <n:NavButton Text="Graph" NavUri="/Pages/Graph.xaml" ImageSource="/Images/cat.jpg" />
            <n:NavButton Text="Charts" NavUri="/Pages/Charts.xaml" ImageSource="/Images/cat.jpg" />
        </WrapPanel>
        
    </Grid>
    
</Page>
    
